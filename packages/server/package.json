{
  "name": "@everything-list/server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "build": "tsc --project .",
    "tsc": "tsc",
    "db:migrate": "tsc && cross-env NODE_ENV=test sequelize db:migrate",
    "lint": "eslint . --ext .ts",
    "start": "node ./dist/index.js --env=production",
    "prestart:dev": "cross-env NODE_ENV=test sequelize db:migrate && yarn cross-env NODE_ENV=test sequelize db:seed:all",
    "start:dev": "nodemon --delay 500ms",
    "pretest": "tsc && cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:no-reloading": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "execMap": {
      "ts": "npx cross-env NODE_ENV=test ts-node --files"
    },
    "ignore": [
      "src/public"
    ]
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/ioredis": "^4.28.10",
    "axios": "^0.26.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-rate-limit": "^6.3.0",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "formdata-node": "^4.3.2",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.3",
    "jet-logger": "^1.1.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.18.0",
    "sharp": "^0.30.4",
    "socket.io": "^4.5.0",
    "typescript": "^4.6.4",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@faker-js/faker": "^6.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/express-session": "^1.17.4",
    "@types/factory-girl": "^5.0.8",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.31",
    "@types/sharp": "^0.30.2",
    "@types/supertest": "^2.0.12",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "factory-girl": "^5.0.4",
    "fs-extra": "^10.0.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsc-hooks": "^1.1.1"
  }
}
